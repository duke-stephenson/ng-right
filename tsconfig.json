{
  "version": "1.6.2",
  "compilerOptions": {
    "noImplicitAny": false,
    "failOnTypeErrors": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "rootDir": ".",
    "outDir": "js",
    "fast": "never",
    "sourceMap": false,
    "noExternalResolve": true
  },
  "exclude": [
    "node_modules",
    "js"
  ]
}
